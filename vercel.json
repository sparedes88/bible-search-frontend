{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "x-vercel-protection-bypass": "true"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "x-vercel-protection-bypass": "true"
      }
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/asset-manifest.json",
      "headers": {
        "x-vercel-protection-bypass": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
